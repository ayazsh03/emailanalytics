@{
    ViewBag.Title = "Success";
}

<h2>Success</h2>
<div class="col-md-12">
    <fieldset>
        <div class="panel panel-success">
            <div class="panel-body">
                <div class="col-md-12" style="text-align:right;">
                    <a href="SearchPackage">Back</a>
                </div>
                <div class="col-md-12">
                    @if (ViewData["Message"] != null)
                    {
                        <label style="font-size:20px; font-weight:bold; color:green;">@ViewData["Message"]</label>
                    }
                </div>
            </div>
        </div>
    </fieldset>
</div>
